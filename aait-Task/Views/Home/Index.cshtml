<script src="https://kendo.cdn.telerik.com/2020.1.406/js/jquery.min.js"></script>
<script src="~/Scripts/bootstrap.min.js"></script>

<script src="https://kendo.cdn.telerik.com/2020.1.406/js/kendo.all.min.js"></script>

<script>
    var dropdownlistdata;
    $(document).ready(function () {
        $.ajax({
            url: "https://dev.aait.com.sa/aait/Service/WebService.asmx/RequestPage_DDL",
            type: "GET",
            success: function (Res) {

                dropdownlistdata = Res
                $("#PDOMAIN_REF").kendoDropDownList({
                    dataTextField: "DOMAIN_NAME",
                    dataValueField: "DOMAIN_ID",
                    dataSource: Res.PDOMAIN_REF,
                    index: 0
                });
                $("#PBUILDING_REF").kendoDropDownList({
                    dataTextField: "BUILDING_NAME",
                    dataValueField: "BUILDING_ID",
                    dataSource: Res.PBUILDING_REF,
                    index: 0
                }); $("#PREQUEST_TYPE_REF").kendoDropDownList({
                    dataTextField: "REQUEST_TYPE_NAME",
                    dataValueField: "REQUEST_TYPE_ID",
                    dataSource: Res.PREQUEST_TYPE_REF,
                    index: 0
                });

                $("#PCATEGORY_MASTER_REF").kendoDropDownList({
                    cascadeFrom: "PDOMAIN_REF",
                    dataTextField: "CATEGORY_MASTER_NAME",
                    dataValueField: "CATEGORY_MASTER_ID",
                    dataSource: Res.PCATEGORY_MASTER_REF,
                    index: 0
                }); $("#PDEPARTMENT_REF").kendoDropDownList({
                    cascadeFrom: "PBUILDING_REF",
                    dataTextField: "DEPARTMENT_NAME",
                    dataValueField: "DEPARTMENT_ID",
                    dataSource: Res.PDEPARTMENT_REF,
                    index: 0
                });

                $("#PCATEGORY_DETAIL_REF").kendoDropDownList({
                    cascadeFrom: "PCATEGORY_MASTER_REF",
                    dataTextField: "CATEGORY_DETAIL_NAME",
                    dataValueField: "CATEGORY_DETAIL_ID",
                    dataSource: Res.PCATEGORY_DETAIL_REF,
                    index: 0
                });
            }

        })





        $("#get").click(function () {
            var categoryInfo = "\nCategory: { id: " + categories.value() + ", name: " + categories.text() + " }",
                productInfo = "\nProduct: { id: " + products.value() + ", name: " + products.text() + " }",
                orderInfo = "\nOrder: { id: " + orders.value() + ", name: " + orders.text() + " }";

            alert("Order details:\n" + categoryInfo + productInfo + orderInfo);
        });


        $("#grid").kendoGrid({
            dataSource: {
                //type: "odata",
                transport: {
                    read: "http://dev.aait.com.sa/aait/Service/WebService.asmx/Request_SEL"
                },
                pageSize: 20
            },
            height: 550,
            toolbar: ["search"],
            serverFiltering: true,
            serverSorting: true,
            filterable: true,
            sortable: true,
            pageable: {
                refresh: true,
                pageSizes: true,
                buttonCount: 5
            },
            columns: [{
                field: "V_DOMAIN_NAME",
                title: "V_DOMAIN_NAME",
            }, {
                field: "CATEGORY_MASTER_NAME",
                title: "CATEGORY_MASTER_NAME"
            },
            {
                field: "CATEGORY_DETAIL_NAME",
                title: "CATEGORY_DETAIL_NAME",
            },
            {
                field: "BUILDING_NAME",
                title: "BUILDING_NAME",
            },
            {
                field: "DEPARTMENT_NAME",
                title: "DEPARTMENT_NAME",
            },
            {
                field: "STATE_NAME",
                title: "STATE_NAME",
            }
            ]

        });
        $("#save").click(function () {
            var Mydata = $("form").serialize();

            $.ajax({
                url: "https://dev.aait.com.sa/aait/Service/WebService.asmx/REQUEST_INS",
                type: "POST",
                dataType: "jsonp",
                data: Mydata,
                headers: {
                    'Access-Control-Allow-Origin': '*',
                },
                success: function (re) {
                    alert(re)
                    debugger
                },
                error: function (a, b, sa) {
                    alert(sa.error)
                    debugger
                },




            })
        
        });
    });


</script>